<%= case @rune.name
    when 'フェオ'
      sanitize '&#x16A0;'
    when 'ウル'
      sanitize '&#x16A2;'
    when 'ソーン'
      sanitize '&#x16A6;'
    when 'アンスル/アンスール'
      sanitize '&#x16A8;'
    when 'ラド'
      sanitize '&#x16B1;'
    when 'ケン'
      sanitize '&#x16B2;'
    when 'ギューフ'
      sanitize '&#x16B7;'
    when 'ウィン'
      sanitize '&#x16B9;'
    when 'ハガル'
      sanitize '&#x16BA;'
    when 'ニイド'
      sanitize '&#x16BE;'
    when 'イス'
      sanitize '&#x16C1;'
    when 'ヤラ'
      sanitize '&#x16C3;'
    when 'ユル'
      sanitize '&#x16C7;'
    when 'ペオス/ペイオース'
      sanitize '&#x16C8;'
    when 'エオロー'
      sanitize '&#x16C9;'
    when 'シゲル'
      sanitize '&#x16CB;'
    when 'ティール'
      sanitize '&#x16CF;'
    when 'ベオーク'
      sanitize '&#x16D2;'
    when 'エオー'
      sanitize '&#x16D6;'
    when 'マン'
        sanitize '&#x16D7;'
    when 'ラーグ'
        sanitize '&#x16DA;'
    when 'イング'
        sanitize '&#x16DD;'
    when 'オセル'
        sanitize '&#x16DF;'
    when 'ダエグ'
        sanitize '&#x16DE;'
    else
      ''
end %>